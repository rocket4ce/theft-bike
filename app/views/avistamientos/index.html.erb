<p id="notice"><%= notice %></p>

<h1>Listing Avistamientos</h1>

<table>
  <thead>
    <tr>
      <th>Bike</th>
      <th>Photo</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @avistamientos.each do |avistamiento| %>
      <tr>
        <td><%= avistamiento.bike %></td>
        <td><%= avistamiento.photo %></td>
        <td><%= avistamiento.address %></td>
        <td><%= avistamiento.latitude %></td>
        <td><%= avistamiento.longitude %></td>
        <td><%= avistamiento.user %></td>
        <td><%= link_to 'Show', avistamiento %></td>
        <td><%= link_to 'Edit', edit_avistamiento_path(avistamiento) %></td>
        <td><%= link_to 'Destroy', avistamiento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Avistamiento', new_avistamiento_path %>
